        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hdmap.data.etl.mapper.TableInfoMapper">

    <resultMap id="BaseResultMap" type="com.hdmap.data.etl.model.entity.TableInfo">
